# Environment variables for Quebec Market Trends MVP
# Copy this file to .env and adjust values as needed

# Project Settings
PROJECT_NAME="Quebec Market Trends Analyzer"
ENVIRONMENT="development"

# Database
DATABASE_PATH="data/trends.db"

# Google Trends (no API key needed, but rate limits apply)
TRENDS_REQUEST_DELAY=2

# Logging
LOG_LEVEL="INFO"
LOG_FILE="logs/trends_collector.log"

# Dashboard
DASHBOARD_PORT=8501
